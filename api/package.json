  "bugs": {
    "url": "https://github.com/2VSummerHack25/SarahFutureUnicorn/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2VSummerHack25/SarahFutureUnicorn.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint --ext=.js src",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "prisma": "npx prisma db push && npx prisma studio",
    "prisma:reset": "npx prisma db push && npx prisma db seed && npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "body-parser": "^2.2.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "lint-staged": "^16.1.4",
    "prettier": "2.8.0",
    "simple-git-hooks": "^2.13.1"
  },
  "prisma": {
    "seed": "node prisma/seed/index.js"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "yarn run format",
    "post-merge": "yarn && npx prisma db push"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix"
  }
}
